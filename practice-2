temperatures = [20,16,19,21,15,22,20]

#Find the sum of all temperatures.
sum = 0
for t in temperatures:
    sum = sum + t

#Find the average temperature
average = sum / len(temperatures)

#Create a range object that will cover all numbers divisible by 3 that are bigger than 14 and smaller than 100.
myRange = range(15, 100, 3)

#Create a list slice that gets all but the last four items in a list
def slice(lst):
    return lst[:-4]



#Create a boolean expression that changes depending on the inclusion of parenthesis!
#False and (True and False) or True -> True
#(False and True) and (False or True) -> False